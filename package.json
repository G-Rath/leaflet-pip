{
  "name": "leaflet-pip",
  "version": "0.1.1",
  "description": "point in polygon searches for leaflet",
  "main": "index.js",
  "scripts": {
    "test": "eslint index.js && browserify test/*.js | tape-run",
    "build-site": "browserify site/site.js > site/bundle.js",
    "prepublish": "browserify -s leafletPip index.js > leaflet-pip.js && uglifyjs leaflet-pip.js -c -m > leaflet-pip.min.js"
  },
  "files": [
    "index.js",
    "leaflet-pip.js",
    "leaflet-pip.min.js"
  ],
  "keywords": [
    "leaflet",
    "plugin",
    "point",
    "in",
    "polygon",
    "geo",
    "geographical"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/mapbox/leaflet-pip.git"
  },
  "devDependencies": {
    "browserify": "~13.0.1",
    "eslint": "^2.2.0",
    "leaflet": "~0.7.3",
    "phantomjs": "~2.1.7",
    "tape": "~4.5.1",
    "tape-run": "~2.1.4"
  },
  "author": "Tom MacWright <tom@macwright.org>",
  "license": "ISC",
  "dependencies": {
    "geojson-utils": "~1.1.0"
  }
}
